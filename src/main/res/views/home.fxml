<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.curator.controllers.HomeController">
   <children>
      <AnchorPane prefWidth="200.0" HBox.hgrow="ALWAYS" />
      <ScrollPane fx:id="mainScrollPane" maxWidth="1500.0" minWidth="-Infinity" prefWidth="1000.0" style="-fx-background-color: #ffffff;">
         <content>
            <VBox fx:id="mainVBox" alignment="TOP_CENTER">
               <children>
                  <VBox fx:id="topRecommendationVBox" alignment="TOP_CENTER" VBox.vgrow="NEVER" />
               </children>
            </VBox>
         </content>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </ScrollPane>
      <AnchorPane prefWidth="200.0" HBox.hgrow="ALWAYS">
         <children>
            <FlowPane prefHeight="200.0" prefWidth="200.0" />
         </children></AnchorPane>
   </children>
</HBox>
