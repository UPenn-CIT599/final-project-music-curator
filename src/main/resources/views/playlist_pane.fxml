<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="playlistPane" prefHeight="200.0" prefWidth="150.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="playlistImage" fitHeight="150.0" fitWidth="130.0" layoutX="9.0" layoutY="15.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="playlistName" layoutX="6.0" layoutY="157.0" prefHeight="14.0" prefWidth="134.0">
         <font>
            <Font size="12.0" />
         </font>
      </Label>
      <ImageView fx:id="playButton" disable="true" fitHeight="50.0" fitWidth="50.0" layoutX="8.0" layoutY="30.0" opacity="0.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../../git/final-project-music-curator/src/main/resources/icons/other/play.png" />
         </image>
      </ImageView>
      <ImageView fx:id="heartButton" disable="true" fitHeight="50.0" fitWidth="50.0" layoutX="86.0" layoutY="30.0" opacity="0.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../../git/final-project-music-curator/src/main/resources/icons/heart.png" />
         </image>
      </ImageView>
      <ImageView fx:id="addToPlaylistButton" disable="true" fitHeight="50.0" fitWidth="50.0" layoutX="8.0" layoutY="93.0" opacity="0.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../../git/final-project-music-curator/src/main/resources/icons/copy.png" />
         </image>
      </ImageView>
      <ImageView fx:id="dislikeButton" disable="true" fitHeight="50.0" fitWidth="50.0" layoutX="86.0" layoutY="100.0" opacity="0.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons/dislike.png" />
         </image>
      </ImageView>
   </children>
</Pane>
